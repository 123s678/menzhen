<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xm.dao.PrescriptiontemplateDao">
    <insert id="addPrescriptiontemplate" parameterType="prescriptiontemplate">
        INSERT INTO `prescriptiontemplate`
        VALUES (
        null,
        #{templatename},
        #{doctorid},
        #{prescriptiontypeid},
        #{classifyid},
        #{note},
        #{diagnosis},
        1
        )
    </insert>
</mapper>
